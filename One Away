string1=input()
string2=input()
len1=len(string1)
len2=len(string2)
if abs(len1-len2)>1:
  print("No")
  exit()
i=j=count=0
if len1>len2 or len1<len2:
  count=1
while i<len1 and j<len2:
  if string1[i]!=string2[j]:
    if count!=0:
      print("No")
      exit()
    count+=1
  i+=1
  j+=1
if count==1:
  print("yes")
else:
  print("no")
  
     
    
